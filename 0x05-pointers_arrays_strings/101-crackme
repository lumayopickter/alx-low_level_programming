#include <stdio.h>
#include <string.h>

int main(int argc, char *argv[])
{
    if (argc != 2)
    {
        printf("Usage: %s [password]\n", argv[0]);
        return 1;
    }

    if (strcmp(argv[1], "pass123") == 0)
    {
        printf("Tada! Congrats\n");
    }
    else
    {
        printf("Wrong password. Try again.\n");
    }

    return 0;
}

